template = 1   # inherit from light healing
mana = 10      # override cost
level = 1      # minimum level for use
flags = 8
param1 = 10    # min damage/amount
param2 = 0     # delta (zero -> fixed)
words = { ex ura }
comment = "Tiny heal 10 (based on spell 1)"

